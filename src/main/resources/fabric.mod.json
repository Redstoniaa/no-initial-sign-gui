{
  "schemaVersion": 1,
  "id": "${id}",
  "version": "${version}",

  "name": "${name}",
  "description": "Cancels the editing screen upon placement of a sign. You can edit it later anyway.",
  "authors": [
    "Redstonia"
  ],
  "contact": {
    "homepage": "https://github.com/Redstoniaa/sign-placement-cancel-editing",
    "sources": "https://github.com/Redstoniaa/sign-placement-cancel-editing"
  },

  "license": "MIT",
  "icon": "assets/spce/icon.png",

  "environment": "client",
  "entrypoints": {
    "client": [
      "spce.SignPlacementCancelEditingMod::init"
    ]
  },
  "mixins": [
    "spce.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.14.19",
    "minecraft": ">=1.20",
    "java": ">=17"
  }
}
